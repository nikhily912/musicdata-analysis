<div class=" highlight hl-ipython3"><pre><span></span><span class="n">chords</span> <span class="o">=</span> <span class="p">[]</span>
<span class="k">for</span> <span class="n">song</span><span class="p">,</span> <span class="n">artist</span> <span class="ow">in</span> <span class="nb">zip</span><span class="p">(</span><span class="n">data</span><span class="p">[</span><span class="s1">&#39;Song&#39;</span><span class="p">],</span> <span class="n">data</span><span class="p">[</span><span class="s1">&#39;Artist&#39;</span><span class="p">]):</span>
    <span class="k">try</span><span class="p">:</span>
        <span class="c1">#get chords with artist and song name</span>
        <span class="nb">print</span><span class="p">(</span><span class="n">get_chords</span><span class="p">(</span><span class="n">song</span><span class="p">,</span> <span class="n">artist</span><span class="p">))</span>
        <span class="n">chords</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">get_chords</span><span class="p">(</span><span class="n">song</span><span class="p">,</span> <span class="n">artist</span><span class="p">))</span>
    <span class="k">except</span><span class="p">:</span>
        <span class="k">try</span><span class="p">:</span>
            <span class="nb">print</span><span class="p">(</span><span class="n">get_chords_no_artist</span><span class="p">(</span><span class="n">song</span><span class="p">))</span>
            <span class="n">chords</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">get_chords_no_artist</span><span class="p">(</span><span class="n">song</span><span class="p">))</span>
        <span class="k">except</span><span class="p">:</span>
            <span class="k">try</span><span class="p">:</span>
                <span class="nb">print</span><span class="p">(</span><span class="n">get_chords_no_artist_echords</span><span class="p">(</span><span class="n">song</span><span class="p">))</span>
                <span class="n">chords</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">get_chords_no_artist_echords</span><span class="p">(</span><span class="n">song</span><span class="p">))</span>
            <span class="k">except</span><span class="p">:</span>
                <span class="nb">print</span><span class="p">(</span><span class="n">np</span><span class="o">.</span><span class="n">NaN</span><span class="p">)</span>
                <span class="n">chords</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">np</span><span class="o">.</span><span class="n">NaN</span><span class="p">)</span>
</pre></div>

