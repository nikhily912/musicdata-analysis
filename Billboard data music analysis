<div class=" highlight hl-ipython3"><pre><span></span><span class="k">def</span> <span class="nf">get_chords</span><span class="p">(</span><span class="n">song</span><span class="p">,</span> <span class="n">artist</span><span class="p">):</span>
    <span class="n">url</span> <span class="o">=</span> <span class="n">get_first_google_search_result</span><span class="p">(</span><span class="n">song</span><span class="p">,</span> <span class="n">artist</span><span class="p">)</span>
