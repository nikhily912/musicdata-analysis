<span class="k">for</span> <span class="n">songchords</span> <span class="ow">in</span> <span class="n">df</span><span class="p">[</span><span class="s1">&#39;Non-Repeating Chords (Not ordered)&#39;</span><span class="p">]:</span>
    <span class="k">for</span> <span class="n">chord</span> <span class="ow">in</span> <span class="n">songchords</span><span class="p">:</span>
        <span class="k">if</span> <span class="n">chord</span> <span class="ow">in</span> <span class="p">[</span><span class="s1">&#39;A&#39;</span><span class="p">,</span><span class="s1">&#39;B&#39;</span><span class="p">,</span><span class="s1">&#39;C&#39;</span><span class="p">,</span><span class="s1">&#39;D&#39;</span><span class="p">,</span><span class="s1">&#39;E&#39;</span><span class="p">,</span><span class="s1">&#39;F&#39;</span><span class="p">,</span><span class="s1">&#39;G&#39;</span><span class="p">]:</span>
            <span class="n">chord_counts</span><span class="p">[</span><span class="n">chord</span><span class="p">]</span> <span class="o">+=</span> <span class="mi">1</span>
<span class="n">chord_counts</span>
